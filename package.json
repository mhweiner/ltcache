{
  "name": "ltcache",
  "private": false,
  "version": "0.0.0-autorel",
  "description": "A lightweight, in-memory caching library - like Redis but much simpler. Features TTL support, concurrent request handling, and comprehensive statistics. Perfect for Node.js applications that need fast caching without the complexity of Redis.",
  "license": "MIT",
  "author": "Marc H. Weiner <mhweiner234@gmail.com> (https://linkedin.com/in/mhweiner)",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.map",
    "!dist/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhweiner/ltcache.git"
  },
  "scripts": {
    "prepare": "npm run build",
    "test": "c8 kizu 'src/**/*.spec.ts' && c8 report -r text -r html",
    "lint": "eslint ./ --ext .js,.ts",
    "build": "tsc"
  },
  "homepage": "https://github.com/mhweiner/ltcache",
  "keywords": [
    "cache",
    "caching",
    "redis-alternative",
    "redis-lite",
    "lightweight-redis",
    "simple-cache",
    "memory-cache",
    "in-memory-cache",
    "ttl",
    "time-to-live",
    "expiration",
    "concurrent-cache",
    "cache-statistics",
    "hit-rate",
    "cache-miss",
    "cache-hit",
    "nodejs-cache",
    "typescript-cache",
    "fast-cache",
    "cache-performance",
    "no-dependencies",
    "zero-config"
  ],
  "devDependencies": {
    "@types/node": "18.19.120",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "c8": "^7.10.0",
    "cjs-mock": "1.4.14",
    "eslint": "^8.4.1",
    "kizu": "3.8.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "jsout": "4.0.5"
  }
}
